<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/catalogo">
    <fragment
        android:id="@+id/catalogo"
        android:name="com.example.eyecoffee.Catalogo"
        android:label="Catalogo" >
        <action
            android:id="@+id/action_catalogo_to_carrinho"
            app:destination="@id/carrinho" />
    </fragment>
    <fragment
        android:id="@+id/carrinho"
        android:name="com.example.eyecoffee.Carrinho"
        android:label="Carrinho" >
        <action
            android:id="@+id/action_carrinho_to_pagamento"
            app:destination="@id/pagamento" />
    </fragment>
    <fragment
        android:id="@+id/pagamento"
        android:name="com.example.eyecoffee.Pagamento"
        android:label="Pagamento" />
</navigation>